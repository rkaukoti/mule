<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.mulesoft.org/schema/mule/core"
      xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd">
    <flow name="defaultFlow">
        <custom-source class="org.mule.test.components.FlowStateTestCase$TestMessageSource"/>
        <echo-component/>
    </flow>
    <flow name="startedFlow" initialState="started">
        <custom-source class="org.mule.test.components.FlowStateTestCase$TestMessageSource"/>
        <echo-component/>
    </flow>
    <flow name="stoppedFlow" initialState="stopped">
        <custom-source class="org.mule.test.components.FlowStateTestCase$TestMessageSource"/>
        <echo-component/>
    </flow>
</mule>
